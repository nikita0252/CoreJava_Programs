package Practice_Ex;
import java.util.*;


public class Demo1 
{
	public static void main(String args[])
	{
		List<String> l = new ArrayList<>();
		l.add("sbi");
		l.add("tring");
		l.add("sanskruti");
		
		String s = l.get(0);
		Iterator<String> itr = l.iterator();
		while(itr.hasNext())
		{
			String st = itr.next();
			if(l.get(0)=="S")
			{
				l.remove(st);
				System.out.println(st);
			}
		}
		
	}

}
